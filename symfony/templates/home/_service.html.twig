<!-- ======= Services Section ======= -->
<section id="services" class="services">
	<div class="container">

		<div class="section-title">
			<h2>Services Web</h2>
			<p>Plus qu'un metier</p>
		</div>

		<div class="row">
			{% for item in services %}


				<div class="col-lg-4 col-md-6 mb-2 d-flex align-items-stretch">
					<div class="icon-box">
						<div class="icon">
							<i class="{{item.class}}"></i>
						</div>
						<h4>
							<a href="">{{item.denomination|capitalize}}</a>
						</h4>
						<p>{{item.description}}</p>
					</div>
				</div>
			{% endfor %}
		</div>

	</div>
</section>
<!-- End Services Section -->
